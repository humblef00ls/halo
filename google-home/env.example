# Copy this to .env and fill in your values
# DO NOT commit .env to git!

# Adafruit IO (same as your ESP32 credentials.h)
ADAFRUIT_IO_USERNAME=your_adafruit_username
ADAFRUIT_IO_KEY=aio_xxxxxxxxxxxxxxxxxxxx

# OAuth settings (use these in Google Actions Console too)
OAUTH_CLIENT_ID=halo-google-home-client
OAUTH_CLIENT_SECRET=generate-a-secure-random-string-here

# Simple login for account linking
HALO_USERNAME=admin
HALO_PASSWORD=change-this-password

